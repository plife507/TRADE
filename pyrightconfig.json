{
  "typeCheckingMode": "basic",
  "pythonVersion": "3.12",
  "include": ["src", "trade_cli.py"],
  "exclude": [
    "src/forge/audits",
    "src/cli/smoke_tests",
    "src/utils/cli_display.py"
  ],
  "extraPaths": ["."],
  "reportMissingImports": false,
  "reportUndefinedVariable": "error",
  "reportPossiblyUnboundVariable": "error"
}
